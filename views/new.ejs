<% include partials/header %>

<h1>Submit a photo</h1>

<form action="/photos" method="POST">
    <label for="title">Title</label>
    <input type="text" id="title" name="photo[title]" placeholder="Photo title here" required>
    <label for="image">Image</label>
    <input type="text" id="image" name="photo[image]" placeholder="Link here" required>
    <label for="date">Date</label>
    <input type="date" id="date" name="photo[date]" placeholder="Date here" required>
    <button type="submit">Add photo</button>
</form>


<% include partials/footer %>